                  total++;
              }else {
                  total = 0;
                  currentIndex++;
                  if(currentIndex >= upList.size()){
                      currentIndex = 0;
                  }
                  server = upList.get(currentIndex); // 从活着的服务中心，获取指定的服务来进行操作
